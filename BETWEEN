-- Ejemplo para obtener datos indicando un rango de fechas
SELECT * FROM MI_TABLA
WHERE FECHACREADO 
BETWEEN TO_DATE('2024-10-01 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
AND TO_DATE('2024-10-03 23:59:59', 'YYYY-MM-DD HH24:MI:SS')
ORDER BY FECHACREADO DESC;

-- Ejemplo optimizado, evita colocar horarios
SELECT * 
FROM MI_TABLA
WHERE FECHACREADO >= TO_DATE('2025-02-01', 'YYYY-MM-DD')
AND FECHACREADO < TO_DATE('2025-02-28', 'YYYY-MM-DD')
ORDER BY FECHACREADO DESC;
